<view><view class="skin-list-container"><block wx:for="{{skinList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="skin-item"><view class="thumbnail"><image class="thumbnail-img" src="{{item.thumbnail}}" mode="widthFix"></image></view><view class="desc"><text class="desc-text">{{item.skin_desc}}</text><button class="{{['status',item.selected?'':'unselected']}}" disabled="{{item.selected}}" data-event-opts="{{[['tap',[['setDefaultSkin',['$0'],[[['skinList','',index,'id']]]]]]]}}" bindtap="__e">{{item.selected?"使用中":"选择"}}</button></view></view></block></view></view>