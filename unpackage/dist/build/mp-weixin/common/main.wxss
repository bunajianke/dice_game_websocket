.custom-button{margin:0 auto;padding:0;position:relative;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;border:1rpx solid #aada85;background:-webkit-linear-gradient(top,#a9f077,#0f920d);background:linear-gradient(180deg,#a9f077,#0f920d);box-shadow:0 0 3rpx 7rpx #4f4f4d,0 0 6rpx 10rpx #595e57}.custom-button::after{content:none}.custom-button::before{content:" ";position:absolute;border-radius:50rpx;bottom:0;left:3%;width:94%;height:50%;background:-webkit-linear-gradient(top,#0e9a09,#0c8f0a);background:linear-gradient(180deg,#0e9a09,#0c8f0a)}.custom-button text{width:100%;color:#fff;font-weight:700;font-style:italic;font-size:14px;z-index:999;text-shadow:-2rpx -2rpx 4rpx #a56d1b}.custom-button:active text{text-shadow:2rpx 2rpx 10rpx #a56d1b}page::after{position:fixed;content:'';left:-1000px;top:-1000px;-webkit-animation:shadow-preload .1s;-webkit-animation-delay:3s;animation:shadow-preload .1s;animation-delay:3s}@-webkit-keyframes shadow-preload{0%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}100%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}}@keyframes shadow-preload{0%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}100%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}}